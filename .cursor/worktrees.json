{
  "worktrees": {
    "wt1": {
      "path": ".cursor/worktrees/docs/wt1",
      "setup": [
        "npm install"
      ],
      "description": "Worktree 1 - Parallel documentation work branch"
    },
    "wt2": {
      "path": ".cursor/worktrees/docs/wt2",
      "setup": [
        "npm install"
      ],
      "description": "Worktree 2 - Parallel documentation work branch"
    },
    "wt3": {
      "path": ".cursor/worktrees/docs/wt3",
      "setup": [
        "npm install"
      ],
      "description": "Worktree 3 - Parallel documentation work branch"
    },
    "wt4": {
      "path": ".cursor/worktrees/docs/wt4",
      "setup": [
        "npm install"
      ],
      "description": "Worktree 4 - Parallel documentation work branch"
    }
  },
  "default-setup": [
    "npm install"
  ],
  "merge-strategy": "auto",
  "conflict-resolution": {
    "strategy": "smart-merge",
    "rules": [
      {
        "condition": "both-sides-modified",
        "action": "merge-content",
        "description": "If both sides have different content, create new content that includes both"
      },
      {
        "condition": "one-side-newer",
        "action": "use-newer",
        "description": "If one side is newer/modified more recently, use that version"
      },
      {
        "condition": "both-sides-same",
        "action": "use-any",
        "description": "If both sides have identical content, use either version"
      }
    ],
    "default-action": "merge-content",
    "preserve-history": true
  },
  "auto-sync": true,
  "sync-interval": 300
}
